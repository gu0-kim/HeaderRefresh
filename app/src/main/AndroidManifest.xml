<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.developergu.refreshmaster">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".app.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ZhiFuBaoActivity"
            android:excludeFromRecents="true"
            android:theme="@style/tranAppTheme" />
        <activity
            android:name=".SimpleDemoActivity"
            android:excludeFromRecents="true"
            android:theme="@style/tranAppTheme" />
        <activity
            android:name=".MainActivity"
            android:excludeFromRecents="true"
            android:theme="@style/tranAppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TestTranActivity"
            android:excludeFromRecents="true"
            android:theme="@style/tranAppTheme"></activity>
    </application>

</manifest>